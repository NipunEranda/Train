<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListedAlarms">

    <RelativeLayout

        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.material.appbar.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/design_default_color_primary"
            android:theme="@style/AppTheme.AppBarOverlay"/>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floatingActionButton"
            android:layout_width="84dp"
            android:layout_height="79dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="29dp"
            android:layout_marginRight="29dp"
            android:layout_marginBottom="35dp"
            android:clickable="true"
            android:src="@android:drawable/ic_input_add"
            app:backgroundTint="@color/colorPrimary"
            app:backgroundTintMode="src_over"
            app:srcCompat="@color/colorAccent" />

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true">

            <TableLayout
                android:id="@+id/mainTable"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#05634E"
                android:padding="4dp"
                android:paddingTop="9dp"
                android:paddingBottom="5dp">

                <TableRow
                    android:id="@+id/TableRow1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingBottom="2dp">

                    <LinearLayout
                        android:id="@+id/layoutRow1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView10"
                            android:layout_width="74dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@color/colorPrimary"
                            android:padding="10dp"
                            app:srcCompat="@drawable/time" />

                        <LinearLayout
                            android:layout_width="287dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/colorPrimary"
                                android:text="Alarm Name 1"
                                android:textColor="@android:color/black"
                                android:textSize="18sp" />

                            <TextView
                                android:id="@+id/textView6"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/colorPrimary"
                                android:text="Gampha to Colombo "
                                android:textColor="@android:color/black"
                                android:textSize="18sp" />

                            <TextView
                                android:id="@+id/textView7"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/colorPrimary"
                                android:text="12.30 - 12.35"
                                android:textColor="@android:color/black"
                                android:textSize="18sp" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/setting"
                            android:layout_width="70dp"
                            android:layout_height="74dp"
                            android:layout_weight="1"
                            android:background="@color/colorPrimary"
                            android:padding="12dp"
                            app:srcCompat="@drawable/sett" />

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="74dp"
                            android:layout_height="74dp"
                            android:layout_weight="1"
                            android:background="@color/colorPrimary"
                            android:padding="12dp"
                            app:srcCompat="@drawable/bin" />
                    </LinearLayout>
                </TableRow>

                <TableRow
                    android:id="@+id/TableRow2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/layoutRow2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView11"
                            android:layout_width="74dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@color/colorPrimary"
                            android:padding="10dp"
                            app:srcCompat="@drawable/time" />

                        <LinearLayout
                            android:layout_width="287dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView8"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/colorPrimary"
                                android:text="Alarm Name 1"
                                android:textColor="@android:color/black"
                                android:textSize="18sp" />

                            <TextView
                                android:id="@+id/textView9"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/colorPrimary"
                                android:text="Gampha to Colombo "
                                android:textColor="@android:color/black"
                                android:textSize="18sp" />

                            <TextView
                                android:id="@+id/textView10"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/colorPrimary"
                                android:text="12.30 - 12.35"
                                android:textColor="@android:color/black"
                                android:textSize="18sp" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/setting1"
                            android:layout_width="70dp"
                            android:layout_height="74dp"
                            android:layout_weight="1"
                            android:background="@color/colorPrimary"
                            android:padding="12dp"
                            app:srcCompat="@drawable/sett" />

                        <ImageView
                            android:id="@+id/imageView4"
                            android:layout_width="74dp"
                            android:layout_height="74dp"
                            android:layout_weight="1"
                            android:background="@color/colorPrimary"
                            android:padding="12dp"
                            app:srcCompat="@drawable/bin" />
                    </LinearLayout>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </TableLayout>
        </androidx.coordinatorlayout.widget.CoordinatorLayout>


    </RelativeLayout>

</FrameLayout>